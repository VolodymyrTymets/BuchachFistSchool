
<!DOCTYPE html>
<html>
<head>

    <title>@ViewBag.Title </title>

    <!--   <meta name="viewport" content="width=device-width" />-->
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/Content/Bootstrap/css")
    @Styles.Render("~/Content/elltments")
       @Styles.Render("~/Content/ContentPage")
    @Styles.Render("~/Content/css")

</head>
<body>

    <canvas class="backroung-canvas" id="backroung_canvas"></canvas>

    <div class="container-fluid">
        <div class="Navigation">
            <nav>
                <ul>
                    <li>
                        <a href="/"> Назад  на головну</a>
                    </li>
                    
                </ul>
            </nav>
            <div class="float-right  login-partial">
                <section id="login">
                    @Html.Partial("_LoginPartial")
                </section>
            </div>
        </div>
   
        <div id="body">
            @{Html.RenderPartial("_message");}
            <div class="container">
                @RenderBody()
            </div>
        </div>

    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/Bootstrap/js")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/background")
    @Scripts.Render("~/bundles/global")
</body>
</html>
